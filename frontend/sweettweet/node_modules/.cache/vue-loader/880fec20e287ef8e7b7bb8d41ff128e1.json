\n\t\t\t\t\t<input  type=\"radio\"\n\t\t\t\t\t\t\tv-model=\"userInfo.gender\" id=\"M\" value=\"M\"> \n\t\t\t\t\t<label for=\"M\">Male</label>\n\t\t\t\t</div>\n\n\t\t\t\t<div class='radio'>\n\t\t\t\t\t<input  type=\"radio\"\n\t\t\t\t\t\t\tv-model=\"userInfo.gender\" id=\"F\" value=\"F\">\n\t\t\t\t\t<label for=\"F\">Female</label>\n\t\t\t\t</div>\n\t\t\t</div>\n\n\t\t\t<div class='field'>\n\t\t\t\t<label>How old are you?</label>\n\t\t\t\t<input class='number' type=\"number\" name=\"age\" value=\"\" v-model=\"userInfo.age\" ref='ageField' min=\"0\" max=\"150\">\n\t\t\t</div>\n\n\t\t\t<div class='field'>\n\t\t\t\t<label>How tall are you</label>\n\t\t\t\t<div id=\"height\">\n\t\t\t\t\t<div>\n\t\t\t\t\t<input type=\"range\" name=\"feet\" min='0' max='7' v-model=\"userInfo.feet\"> \n\t\t\t\t\t<label for=\"feet\">{{userInfo.feet}} ft</label>\n\t\t\t\t\t</div>\n\n\t\t\t\t\t<div >\n\t\t\t\t\t<input type=\"range\" name=\"inch\" min='0' max='11' v-model=\"userInfo.inch\">\n\t\t\t\t\t<label for=\"inch\">{{userInfo.inch}} in</label>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\n\t\t\t</div>\n\n\t\t\t<div class='field'>\n\t\t\t\t<label>How much do you weigh in lbs?</label>\n\t\t\t\t<input class='number' type=\"number\" name=\"weight\" value=\"\" v-model=\"userInfo.weight\" ref='weightField' min=\"0\" max=\"600\">\n\t\t\t</div>\n\n\t\t\t<div class='field'>\n\t\t\t\t<label>How do you get your insulin?</label>\n\t\t\t\t<div class='radio'>\n\t\t\t\t\t<input  type=\"radio\"\n\t\t\t\t\t\t\tv-model=\"userInfo.insulinDelivery\" id=\"Pump\" value=\"pump\"> \n\t\t\t\t\t<label for=\"Pump\">Pump</label>\n\t\t\t\t</div>\n\n\t\t\t\t<div class='radio'>\n\t\t\t\t\t<input  type=\"radio\"\n\t\t\t\t\t\t\tv-model=\"userInfo.insulinDelivery\" id=\"Injection\" value=\"injection\">\n\t\t\t\t\t<label for=\"Injection\">Injection</label>\n\t\t\t\t</div>\n\t\t\t</div>\n\n\t\t</form>\n<!-- \t\t<div v-if=\"success\" \n\t\t\t\tclass=\"success-message\">\n\t\t<p> -->\n<!-- Sending alerts to {{phoneNumber}}\n  </p> -->\n\n  </div>\n</template>\n\n<script>\n\texport default {\n\t\tname : 'user-form',\n\t\tdata() {\n\t\t\treturn {\n\t\t\t\tsubmitting : false,\n\t\t\t\terror : false,\n\t\t\t\tsuccess: false,\n\t\t\t\tempty: true,\n\t\t\t\tuserInfo: {gender: '',\n\t\t\t\t\t\t\tage: '',\n\t\t\t\t\t\t\tfeet: 0,\n\t\t\t\t\t\t\tinch: 0,\n\t\t\t\t\t\t\tweight: '',\n\t\t\t\t\t\t\tinsulinDelivery: '',\n\t\t\t\t\t\t\t},\n\t\t\t\tage_empty: true,\n\t\t\t\tfeet: 0,\n\t\t\t\tinch: 0,\n\t\t\t\tfull_user_info: false\n\t\t\t}\n\t\t},\n\n\t\t// props: {\n\t\t// \tgender: String,\n\t\t// },\n\n\t\tmethods: {\n\t\t\thandleSubmit() {\n\t\t\t\t// console.log('Testing handleSubmit in phone-form');\n\t\t\t\tthis.submitting = true\n\t\t\t\t// this.clearStatus()\n\n\t\t\t\tthis.$emit('add:userInfo', this.userInfo)\n\t\t\t\tthis.error = false\n\t\t\t\tthis.success = true\n\t\t\t\tthis.submitting = false\n\t\t\t\t// this.empty = false\n\t\t\t\t// this.gender = ''\n\t\t\t},\n\n\t\t\t// clearStatus() {\n\t\t\t// \tthis.success = false\n\t\t\t// \tthis.error = false\n\t\t\t// },\n\n\t\t\thandleChangeUserInfo() {\n\t\t\t\t// this.clearStatus()\n\t\t\t\t// this.userInfo = {gender: '',\n\t\t\t\t// \t\t\tage: '',\n\t\t\t\t// \t\t\tfeet: 0,\n\t\t\t\t// \t\t\tinch: 0,\n\t\t\t\t// \t\t\tweight: 0,\n\t\t\t\t// \t\t\tinsulinDelivery: '',\n\t\t\t\t// \t\t\t}\n\t\t\t\t// this.$emit('add:newPhoneNumber', this.newPhoneNumber)\n\t\t\t\tthis.full_user_info = false\n\t\t\t\t// this.$refs.phoneField.focus()\n\t\t\t},\n\t\t\tgoToAge() {\n\t\t\t\tthis.$refs.ageField.focus()\n\t\t\t},\n\t\t},\n\t\tcomputed: {\n\t\t\tgender_empty() {\n\t\t\t\treturn this.userInfo.gender == ''\n\t\t\t},\n\t\t},\n\t}\n</script>\n\n<style scoped>\n#phone_form {\n\tmargin-bottom: 5em;\n}\nform {\n\tmargin: auto;\n\twidth: 50%;\n}\n.radio {\n\tdisplay: flex;\n\tflex-direction: row;\n\talign-items: baseline;\n\tjustify-content: center;\n}\n#height {\n\tmargin: auto;\n\twidth: 300px;\n\tdisplay: flex;\n\tjustify-content: space-between;\n}\n.number {\n\twidt