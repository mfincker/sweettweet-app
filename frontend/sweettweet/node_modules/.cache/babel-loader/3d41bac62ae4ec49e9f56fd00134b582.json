AA;AACA,KAxBA;AA0BA,IAAA,uBA1BA,qCA0BA;AACA,WAAA,WAAA;AACA,WAAA,cAAA,GAAA,EAAA;AACA,WAAA,KAAA,CAAA,oBAAA,EAAA,KAAA,cAAA;AACA,WAAA,KAAA,GAAA,IAAA,CAJA,CAKA;AACA,KAhCA;AAkCA,IAAA,iBAlCA,+BAkCA;AACA,WAAA,iBAAA,GAAA,CAAA,KAAA,iBAAA;AACA;AApCA,GAjBA;AAuDA,EAAA,QAAA,EAAA;AACA,IAAA,YADA,0BACA;AACA,UAAA,OAAA,GAAA,eAAA;AACA,aAAA,KAAA,cAAA,CAAA,KAAA,CAAA,OAAA,KAAA,IAAA;AACA;AAJA;AAvDA,CAAA","sourcesContent":["<!-- Form to add/update user phone number -->\n<!-- emit: add:newPhoneNumber -->\n\n<template>\n\t<div id='phone-form'>\n\t\t<div class='hgroup'>\n\t\t<h3>Phone number</h3>\n\t\t<button id='phone-num-mod' v-if=\"!show_phone_number\" @click=\"togglePhoneNumber\">Modify</button>\n\t\t</div>\n\t\t<form @submit.prevent=\"handleSubmit\"\n\t\t\t\tv-if=\"empty\">\n\t\t\t<label>Enter your phone number below to receive hypoglycemia alerts:</label>\n\t\t\t<input  type=\"text\"\n\t\t\t\t\t:class=\"{ 'has-error': submitting && invalidPhone }\"\n\t\t\t\t\tv-model=\"newPhoneNumber\"\n\t\t\t\t\t@focus=\"clearStatus\"\n\t\t\t\t\t@keypress=\"clearStatus\"\n\t\t\t\t\tref=\"phoneField\">\n\t\t\t<p v-if=\"error && submitting\" class=\"error-message\">‚ùóPlease fill out your phone number as +10123456789\n\t\t\t</p>\n\t\t\t<button>Submit</button>\n\t\t</form>\n\t\t<div v-if=\"success\" \n\t\t\t\tclass=\"success-message\">\n\t\t<p>\nSending alerts to {{phoneNumber}}\n  </p>\n  <button @click=\"handleChangePhoneNumber\">Change phone number</button>\n  </div>\n\t</div>\n</template>\n\n<script>\n\texport default {\n\t\tname : 'phone-form',\n\t\tdata() {\n\t\t\treturn {\n\t\t\t\tsubmitting : false,\n\t\t\t\terror : false,\n\t\t\t\tsuccess: false,\n\t\t\t\tnewPhoneNumber : '',\n\t\t\t\tempty: true ,\n\t\t\t\tshow_phone_number: true\n\t\t\t}\n\t\t},\n\n\t\tprops: {\n\t\t\tphoneNumber: String,\n\t\t},\n\n\t\tmethods: {\n\t\t\thandleSubmit() {\n\t\t\t\t// console.log('Testing handleSubmit in phone-form');\n\t\t\t\tthis.submitting = true\n\t\t\t\tthis.clearStatus()\n\n\t\t\t\tif (this.invalidPhone) {\n\t\t\t\t\tthis.error = true\n\t\t\t\t\tthis.$refs.phoneField.focus()\n\t\t\t\t\treturn\n\t\t\t\t}\n\n\t\t\t\tthis.$emit('add:newPhoneNumber', this.newPhoneNumber)\n\t\t\t\tthis.error = false\n\t\t\t\tthis.success = true\n\t\t\t\tthis.submitting = false\n\t\t\t\tthis.empty = false\n\t\t\t\tthis.newPhoneNumber = ''\n\t\t\t\tthis.togglePhoneNumber()\n\t\t\t},\n\n\t\t\tclearStatus() {\n\t\t\t\tthis.success = false\n\t\t\t\tthis.error = false\n\t\t\t},\n\n\t\t\thandleChangePhoneNumber() {\n\t\t\t\tthis.clearStatus()\n\t\t\t\tthis.newPhoneNumber = ''\n\t\t\t\tthis.$emit('add:newPhoneNumber', this.newPhoneNumber)\n\t\t\t\tthis.empty = true\n\t\t\t\t// this.$refs.phoneField.focus()\n\t\t\t},\n\n\t\t\ttogglePhoneNumber() {\n\t\t\t\tthis.show_phone_number = ! this.show_phone_number\n\t\t\t}\n\t\t},\n\t\tcomputed: {\n\t\t\tinvalidPhone() {\n\t\t\t\tlet phoneno = /^\\+\\d{11,13}$/;\n\t\t\t\treturn this.newPhoneNumber.match(phoneno) == null\n\t\t\t},\n\t\t},\n\t}\n</script>\n\n<style scoped>\n\t#phone_form {\n\t\tmargin-bottom: 5em;\n\t}\n  form {\n    margin: auto;\n    width: 50%;\n  }\n</style>"],"sourceRoot":"src/components"}]}